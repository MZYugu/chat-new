<div class="side_bar_header">
  <div class="header_name">
    <%= link_to current_user.name, edit_user_path(current_user.id) %>
  </div>
  <div class="create_room">
    <%= link_to 'CREATE CHAT', new_room_path %>
  </div>
</div>

<div class="rooms">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room_name">
        <a href="#"><%= room.name %></a>
      </div>
    </div>
  <% end %>
</div>